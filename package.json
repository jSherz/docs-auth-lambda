{
  "name": "docs-auth-lambda",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "prepackage": "rm -rf dist/* function.zip && yarn build && NODE_ENV=production yarn",
    "package": "cp -r public node_modules dist/ && cd dist && zip -r ../function.zip .",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prettify": "prettier --write 'src/**/*.ts'"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.3",
    "@types/cookie": "^0.3.1",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.1.1",
    "@types/qs": "^6.5.1",
    "jest": "^22.4.3",
    "moment": "^2.22.1",
    "prettier": "^1.13.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie": "^0.3.1",
    "jsonwebtoken": "^8.2.1",
    "qs": "^6.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/__tests__/*"
    ],
    "coveragePathIgnorePatterns": [
      "src/vars-example.ts",
      "src/services/index.ts"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
